{
  "name": "bytes-to-co2",
  "version": "0.3.129",
  "description": "Javascript module that calculates the co2 generated in transfer x amount of bytes over the network and the country.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "umd": "dist/umd/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "repository": {
    "url": "https://github.com/dvelasquez/carbon-tools",
    "type": "git"
  },
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "rollup -c"
  },
  "author": "Danilo Velasquez <danilo.velasquez@gmail.com>",
  "license": "MIT",
  "keywords": [
    "carbon footprint",
    "co2",
    "sustainability",
    "cost of transfer data"
  ],
  "tap": {
    "nyc-arg": [
      "--exclude=**/__mocks__/*.*"
    ],
    "reporter": "spec",
    "no-ts": true,
    "test-env": [
      "TS_NODE_PROJECT=tsconfig.json",
      "TS_NODE_FILES=true"
    ]
  },
  "dependencies": {
    "co2-data": "workspace:^0.0.127"
  }
}
